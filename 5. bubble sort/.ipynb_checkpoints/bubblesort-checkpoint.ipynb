    "def bubblesort(db):\n",
    "    for i in range(len(db)-1):\n",
    "        for j in range(len(db)-1-i):\n",
    "            if(db[j]>db[j+1]):\n",
    "                db[j],db[j+1]=db[j+1],db[j]\n",
    "    return db\n",
    "                \n",
    "        "
   ]
